{
	"name": "search",
	"description": "The search engine in your codebase",
	"icon": "img/icon.png",
	"publisher": "mintlify",
	"displayName": "Mintlify",
	"version": "0.0.3",
	"repository": {
		"url": "https://github.com/mintlify/vscode"
	},
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:mintlify.search",
		"onCommand:mintlify.ask",
		"onCommand:mintlify.logout",
		"onCommand:mintlify.settings"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "mintlify.search",
				"title": "Mint Search"
			},
			{
				"command": "mintlify.ask",
				"title": "Mint Ask"
			},
			{
				"command": "mintlify.logout",
				"title": "Logout of Mintlify"
			},
			{
				"command": "mintlify.settings",
				"title": "Open Mintlify Settings"
			}
		],
		"keybindings": [
			{
				"key": "ctrl+;",
				"mac": "cmd+;",
				"command": "mintlify.search"
			},
			{
				"key": "ctrl+'",
				"mac": "cmd+'",
				"command": "mintlify.ask"
			}
		],
		"productIconThemes": [
			{
				"id": "mintlify-icons",
				"label": "Mintlify",
				"path": "./icons/mintlify-product-icon-theme.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"local": "vsce package && code --install-extension search-0.0.3.vsix"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.3",
		"webpack": "^5.64.2",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"url": "^0.11.0",
		"vsce": "^2.3.0"
	}
}
